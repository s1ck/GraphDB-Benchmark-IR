# properties for embedded OrientDB instance
location=out/orientdb

# sets the threshold for number of outgoing edges
min_outdegree=20

# log benchmark results out directory
log=true


### Import 

# select if the dataset shall be transfered from mysql
import=true
# dataset which will be imported from mysql
database=deu_news_2009_10K

### Benchmarks

# perform a warmup before running a benchmark
warmup=true
# select the queries to run 
query1_native=true
query2_native=true
query3_native=true